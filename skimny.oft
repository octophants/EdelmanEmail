		.header,.footer{
			background-color:#0062cf;
			color:#ffffff;
			text-align:center;
		}
		.footer{
			height:80px;
			font-weight:100;;
			width: 1200px;
		}
		h2{
			font-weight:700;
			letter-spacing:3px;
			font-size:10pt;
			padding-bottom:15px;
		}
		.viewwork{
			color:#fff;
			border:2px solid #fff;
			padding:10px;
			font-size:10pt;
		}
		.viewwork:hover{
			text-shadow:none;
		}
		a{
			text-decoration:none;
		}

		.banner{
			width: 100%;
		}


		@media screen and (max-width: 650px) {
			.header{
				font-size: 14pt;
			}

			.footer{
				font-size: 10pt;
			}

			h1{
				font-size: 24pt;
				margin-left: 30px;
				margin-right: 30px;
			}

			p{
				font-size: 14pt;
				width: 90%;
				margin-left: 5%;
				margin-right: 5%;
				
			}

			.banner{
				width: 250%;
				margin-left: -75%;
			}



        }



        }

</style></head>
